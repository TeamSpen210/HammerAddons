// currently using game_score's iconsprite
@PointClass base(BaseEntity,TeamNum) appliesto(DODS) iconsprite("editor/ts2do/game_score.vmt") = dod_scoring : "Custom Scoring"
[
	point_give_delay(integer) : "Seconds between point gives" : 60
	point_give_amount(integer) : "Points to give each time" : 1
